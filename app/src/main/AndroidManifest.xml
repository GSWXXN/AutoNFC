    xmlns:tools="http://schemas.android.com/tools"
    package="com.gswxxn.autonfc">

    <application
        android:label="Auto NFC"
        tools:ignore="MissingApplicationIcon">

        <meta-data
            android:name="xposedmodule"
            android:value="true" />

        <meta-data
